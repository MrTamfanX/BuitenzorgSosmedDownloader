<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/bg"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="15dp"
		android:paddingLeft="10dp"
		android:paddingTop="8dp"
		android:paddingRight="10dp"
		android:paddingBottom="8dp"
		android:gravity="center_horizontal"
		android:orientation="vertical">
		<TextView
			android:id="@+id/menu1"
			android:focusable="false"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingTop="2dp"
			android:paddingRight="8dp"
			android:paddingBottom="8dp"
			android:text="Menu Yang Tersedia Saat Ini"
			android:textSize="16sp"
			android:textStyle="bold"
			android:textColor="#00796B"
			android:layout_gravity="center_horizontal|center_vertical"/>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/animation1"
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/i1"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/ic_buitenzsyndicate_29"
					android:scaleType="fitCenter"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/animation2"
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:layout_marginLeft="10dp"
				android:layout_marginRight="10dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/i2"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/ic_buitenzsyndicate_9"
					android:scaleType="fitCenter"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/animation3"
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/i3"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/ic_buitenzsyndicate_20"
					android:scaleType="fitCenter"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear4"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingTop="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/t1"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="5dp"
				android:padding="10dp"
				android:background="#009688"
				android:gravity="center_horizontal|center_vertical"
				android:text="WhatsApp Messen"
				android:textSize="14sp"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
			<TextView
				android:id="@+id/t2"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="10dp"
				android:background="#009688"
				android:gravity="center_horizontal|center_vertical"
				android:text="WhatsApp Business"
				android:textSize="14sp"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear5"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="10dp"
			android:layout_marginBottom="10dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/t3"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="5dp"
				android:padding="10dp"
				android:background="#1A237E"
				android:gravity="center_horizontal|center_vertical"
				android:text="Instagram F &amp; V"
				android:textSize="14sp"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
			<TextView
				android:id="@+id/t4"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="10dp"
				android:background="#D32F2F"
				android:gravity="center_horizontal|center_vertical"
				android:text="YouTube MP3 &amp; MP4"
				android:textSize="14sp"
				android:textColor="#FFFFFF"/>
		</LinearLayout>
		<TextView
			android:id="@+id/t5"
			android:focusable="false"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:padding="10dp"
			android:background="#0D47A1"
			android:gravity="center_horizontal|center_vertical"
			android:text="Facebook Video"
			android:textSize="14sp"
			android:textColor="#FFFFFF"/>
		<TextView
			android:id="@+id/t6"
			android:focusable="false"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="10dp"
			android:padding="10dp"
			android:background="#2196F3"
			android:gravity="center_horizontal|center_vertical"
			android:text="MediaFire Downloader"
			android:textSize="14sp"
			android:textColor="#FFFFFF"/>
		<TextView
			android:id="@+id/t7"
			android:focusable="false"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="10dp"
			android:padding="10dp"
			android:background="#E91E63"
			android:gravity="center_horizontal|center_vertical"
			android:text="Berbagi Aplikasi Ke Teman"
			android:textSize="14sp"
			android:textColor="#FFFFFF"/>
	</LinearLayout>
</LinearLayout>
