<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/bg"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="15dp"
			android:layout_marginTop="60dp"
			android:layout_marginRight="15dp"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="3dp"
				android:padding="8dp"
				android:background="#D50000"
				android:orientation="horizontal"/>
			<TextView
				android:id="@+id/t1"
				android:focusable="false"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="17dp"
				android:layout_marginTop="47dp"
				android:layout_marginRight="17dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Anda Mau Keluar \?"
				android:textSize="16sp"
				android:textColor="#000000"/>
			<TextView
				android:id="@+id/t2"
				android:focusable="false"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginLeft="17dp"
				android:layout_marginTop="10dp"
				android:layout_marginRight="17dp"
				android:layout_marginBottom="10dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Jika anda mau keluar dari aplikasi ini anda bisa menekan &quot;Saya Keluar&quot; atau jika tidak anda bisa menekan &quot;Tidak Jadi&quot;."
				android:textSize="14sp"
				android:textColor="#616161"/>
			<LinearLayout
				android:id="@+id/linear3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="17dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/b1"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:background="#F5F5F5"
					android:gravity="center_horizontal|center_vertical"
					android:text="Tidak Jadi"
					android:textSize="14sp"
					android:textColor="#616161"
					android:layout_weight="1"/>
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="17dp"
					android:layout_height="0dp"
					android:padding="8dp"
					android:orientation="vertical"/>
				<TextView
					android:id="@+id/b2"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:background="#F44336"
					android:gravity="center_horizontal|center_vertical"
					android:text="Saya Keluar"
					android:textSize="14sp"
					android:textColor="#FFFFFF"
					android:layout_weight="1"/>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/b3"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:background="#D50000"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical"
			android:translationY="-260.0dp">
			<ImageView
				android:id="@+id/i1"
				android:focusable="false"
				android:layout_width="60dp"
				android:layout_height="60dp"
				android:padding="6dp"
				android:src="@drawable/sign_out_white_drawable"
				android:scaleType="centerInside"/>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
